{
  "schemaVersion": 1,
  "id": "onthequest",
  "version": "${version}",
  "name": "OnTheQuest",
  "description": "A custom client-side Minecraft mod for the QuestSMP server.",
  "authors": [
    "BluSpring"
  ],
  "contact": {
    "repo": "https://github.com/BluSpring/Kilt"
  },
  "license": "LGPL-3.0",
  "icon": "assets/onthequest/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "xyz.bluspring.onthequest.client.OnTheQuestFabricClient"
    ],
    "main": [
      "xyz.bluspring.onthequest.OnTheQuestFabric"
    ],
    "modmenu": [
      "xyz.bluspring.onthequest."
    ]
  },
  "mixins": [
    "OnTheQuest.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fabric-language-kotlin": ">=1.9.4+kotlin.1.8.21",
    "cloth-config": ">=${cloth_config_version}"
  }
